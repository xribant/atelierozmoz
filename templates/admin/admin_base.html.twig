<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8"/>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>

		<title>Ôzmoz Admin |
			{% block title %}{% endblock %}
		</title>

		<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport'/>
		<meta
		name="viewport" content="width=device-width"/>


		<!-- Bootstrap core CSS     -->
		<link
		href="{{ asset('assets/admin/css/bootstrap.min.css') }}" rel="stylesheet"/>

		<!--  Paper Dashboard core CSS    -->
		<link
		href="{{ asset('assets/admin/css/paper-dashboard.css') }}" rel="stylesheet"/>

		<!--  Fonts and icons     -->
		<link href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
		<link href='https://fonts.googleapis.com/css?family=Muli:400,300' rel='stylesheet' type='text/css'>
		<link href="{{ asset('assets/admin/css/themify-icons.css') }}" rel="stylesheet">
	</head>

	<body>
		<div class="wrapper">
			<div
				class="sidebar" data-background-color="brown" data-active-color="danger">
				<!--
							Tip 1: you can change the color of the sidebar's background using: data-background-color="white | brown"
							Tip 2: you can change the color of the active button using the data-active-color="primary | info | success | warning | danger"
						-->
				<div class="logo">
					<a href="{{ path('admin_home') }}" class="simple-text logo-mini">
						AÔz
					</a>

					<a href="{{ path('admin_home') }}" class="simple-text logo-normal">
						ADMIN Ôzmoz
					</a>
				</div>
				<div class="sidebar-wrapper">
					<div class="user">
						<div class="info">
							<a data-toggle="collapse" href="#collapseExample" class="collapsed">
								<span>
									{{ app.user.firstName }}
									{{ app.user.lastName }}
									<b class="caret"></b>
								</span>
							</a>
							<div class="clearfix"></div>

							<div class="collapse" id="collapseExample">
								<ul class="nav">
									<li>
										<a href="{{ path('app_logout') }}">
											<span class="sidebar-mini">OUT</span>
											<span class="sidebar-normal">Déconnexion</span>
										</a>
									</li>
								</ul>
							</div>
						</div>
					</div>
					<ul class="nav">

						<li {% if active_menu == "dashboard" %} class="active" {% endif %}>
							<a href="{{ path('admin_home') }}">
								<i class="ti-panel"></i>
								<p>Dashboard</p>
							</a>
						</li>
						<li {% if active_menu == 'utilisateurs' %} class="active" {% endif %}>
							<a href="{{ path('app_user_index') }}">
								<i class="ti-user"></i>
								<p>Utilisateurs</p>
							</a>
						</li>
						<li {% if active_menu == 'ateliers' %} class="active" {% endif %}>
							<a href="{{ path('app_workshop_index') }}">
								<i class="ti-palette"></i>
								<p>Ateliers</p>
							</a>
						</li>
						<li {% if active_menu == 'formations' %} class="active" {% endif %}>
							<a href="{{ path('app_training_index') }}">
								<i class="ti-medall-alt"></i>
								<p>Formations</p>
							</a>
						</li>
						<li {% if active_menu == 'coaching' %} class="active" {% endif %}>
							<a href="{{ path('app_coaching_index') }}">
								<i class="ti-comments"></i>
								<p>Coaching</p>
							</a>
						</li>
						<li {% if active_menu == 'localisations' %} class="active" {% endif %}>
							<a href="{{ path('app_workshop_location_index') }}">
								<i class="ti-location-pin"></i>
								<p>Localisations</p>
							</a>
						</li>
						<li {% if active_menu == 'testimonial' %} class="active" {% endif %}>
							<a href="{{ path('app_testimonial_index') }}">
								<i class="ti-heart"></i>
								<p>Témoignages</p>
							</a>
						</li>
						<li {% if active_menu == 'agenda' %} class="active" {% endif %}>
							<a href="{{ path('app_event_index') }}">
								<i class="ti-calendar"></i>
								<p>Agenda</p>
							</a>
						</li>
						<li {% if active_menu == 'registration' %} class="active" {% endif %}>
							<a href="{{ path('app_event_registration_index') }}">
								<i class="ti-marker-alt"></i>
								<p>Inscriptions</p>
							</a>
						</li>
						<li>
							<a data-toggle="collapse" href="#tablesExamples" {% if active_menu == 'newsletter_subscribers' %} aria-expanded="true" {% endif %}>
								<i class="ti-email"></i>
								<p>
									Newsletter
									<b class="caret"></b>
								</p>
							</a>
							<div {% if active_menu == 'newsletter_subscribers' %} class="collapse in" {% else %} class="collapse" {% endif %} id="tablesExamples">
								<ul class="nav">
									<li {% if active_menu == 'newsletter_subscribers' %} class="active" {% endif %}>
										<a href="{{ path('app_news_subscriber_index') }}">
											<span class="sidebar-mini">AB</span>
											<span class="sidebar-normal">Abonnés</span>
										</a>
									</li>
									<li>
										<a href="#">
											<span class="sidebar-mini">EV</span>
											<span class="sidebar-normal">Envoi</span>
										</a>
									</li>
								</ul>
							</div>
						</li>

					</ul>

				</div>
			</div>

			<div class="main-panel">
				<nav class="navbar navbar-default">
					<div class="container-fluid">
						<div class="navbar-minimize">
							<button id="minimizeSidebar" class="btn btn-fill btn-icon">
								<i class="ti-more-alt"></i>
							</button>
						</div>
						<div class="navbar-header">
							<button type="button" class="navbar-toggle">
								<span class="sr-only">Toggle navigation</span>
								<span class="icon-bar bar1"></span>
								<span class="icon-bar bar2"></span>
								<span class="icon-bar bar3"></span>
							</button>
							<a class="navbar-brand" href="#"> {% block pageTitle %}{% endblock %}
								</a>
							</div>
						</div>
					</nav>

					{% block content %}{% endblock %}

					<footer class="footer">
						<div class="container-fluid">
							<nav class="pull-left">
								<ul>
									<li><ahref="{{ path('app_home') }}" target="_blank">Accueil Ôzmoz</a></li>
								</ul>
							</nav>
							<div class="copyright pull-right">
								&copy;
								<script>
									document.write(new Date().getFullYear())
								</script>, réalisé par
								<a href="https://www.mywebcreation.be" target="_blank">My Web Creation</a>
							</div>
						</div>
					</footer>
				</div>
			</div>
		</body>

		<!--   Core JS Files. Extra: TouchPunch for touch library inside jquery-ui.min.js   -->
		<script src="{{ asset('assets/admin/js/jquery.min.js') }}" type="text/javascript"></script>
		<script src="{{ asset('assets/admin/js/jquery-ui.min.js') }}" type="text/javascript"></script>
		<script src="{{ asset('assets/admin/js/perfect-scrollbar.min.js') }}" type="text/javascript"></script>
		<script src="{{ asset('assets/admin/js/bootstrap.min.js') }}" type="text/javascript"></script>

		<!--  Forms Validations Plugin -->
		<script src="{{ asset('assets/admin/js/jquery.validate.min.js') }}"></script>

		<!-- Sliders Plugin -->
		<script src="{{ asset('assets/admin/js/nouislider.min.js') }}"></script>

		<!-- Promise Library for SweetAlert2 working on IE -->
		<script src="{{ asset('assets/admin/js/es6-promise-auto.min.js') }}"></script>

		<!--  Plugin for Date Time Picker and Full Calendar Plugin-->
		<script src="{{ asset('assets/admin/js/moment.min.js') }}"></script>

		<!--  Date Time Picker Plugin is included in this js file -->
		<script src="{{ asset('assets/admin/js/bootstrap-datetimepicker.js') }}"></script>
		<script src="{{ asset('assets/admin/js/locales/fr.js') }}"></script>

		<!--  Select Picker Plugin -->
		<script src="{{ asset('assets/admin/js/bootstrap-selectpicker.js') }}"></script>

		<!--  Switch and Tags Input Plugins -->
		<script src="{{ asset('assets/admin/js/bootstrap-switch-tags.js') }}"></script>

		<!-- Circle Percentage-chart -->
		<script src="{{ asset('assets/admin/js/jquery.easypiechart.min.js') }}"></script>

		<!--  Charts Plugin -->
		<script src="{{ asset('assets/admin/js/chartist.min.js') }}"></script>

		<!--  Notifications Plugin    -->
		<script src="{{ asset('assets/admin/js/bootstrap-notify.js') }}"></script>

		<!-- Sweet Alert 2 plugin -->
		<script src="{{ asset('assets/admin/js/sweetalert2.js') }}"></script>

		<!-- Vector Map plugin -->
		<script src="{{ asset('assets/admin/js/jquery-jvectormap.js') }}"></script>

		<!-- Wizard Plugin    -->
		<script src="{{ asset('assets/admin/js/jquery.bootstrap.wizard.min.js') }}"></script>

		<!--  Bootstrap Table Plugin    -->
		<script src="{{ asset('assets/admin/js/bootstrap-table.js') }}"></script>

		<!--  Plugin for DataTables.net  -->
		<script src="{{ asset('assets/admin/js/jquery.datatables.js') }}"></script>

		<!--  Full Calendar Plugin    -->
		<script src="{{ asset('assets/admin/js/fullcalendar.min.js') }}"></script>

		<!-- Paper Dashboard PRO Core javascript and methods for Demo purpose -->
		<script src="{{ asset('assets/admin/js/paper-dashboard.js') }}"></script>

		{% block javascripts %}{% endblock %}

		<script type="text/javascript">

			$(function () {
$('.datepicker').datetimepicker({locale: 'fr', format: 'L'});
});

$(function () {
$('.datetimepicker').datetimepicker({
locale: 'fr',
format: 'DD/MM/YYYY HH:mm',
icons: {
time: "fa fa-clock-o",
date: "fa fa-calendar",
up: "fa fa-chevron-up",
down: "fa fa-chevron-down",
previous: 'fa fa-chevron-left',
next: 'fa fa-chevron-right',
today: 'fa fa-screenshot',
clear: 'fa fa-trash',
close: 'fa fa-remove'
}
});
});
		</script>

	</html>

